{
  "name": "flowbuilder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "prebuild": "echo 'Skipping auto-fix (has bugs) - use npm run check:fix manually if needed'",
    "build": "vite build",
    "build:server": "echo 'Server uses runtime tsx - no build needed'",
    "build:server:legacy": "tsc -p tsconfig.server.build.json",
    "start:server": "tsx server/index.ts",
    "init-admin": "tsx server/init-admin.js",
    "preview": "vite preview --port 5173",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:integration": "vitest run --dir tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "ts-prune": "ts-prune",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "check": "tsx scripts/pre-deploy-check.ts",
    "check:fix": "tsx scripts/auto-fix-awaits.ts && npm run tsc"
  },
